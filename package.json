{
  "name": "antv-unfold",
  "version": "1.1.0",
  "description": "一个基于AntV G6的可配置关系图可视化库，支持节点展开功能。",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "examples",
    "README.md"
  ],
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "test": "node test/run-tests.js",
    "test:jest": "jest",
    "prepublishOnly": "npm run build && npm test",
    "dev": "webpack --mode=development --watch"
  },
  "keywords": [
    "graph",
    "visualization",
    "antv",
    "g6",
    "relationship",
    "network",
    "chart",
    "unfold",
    "expand",
    "node"
  ],
  "author": "Prog77X",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Prog77X/antv-unfold.git"
  },
  "bugs": {
    "url": "https://github.com/Prog77X/antv-unfold/issues"
  },
  "homepage": "https://github.com/Prog77X/antv-unfold#readme",
  "dependencies": {
    "@antv/g6": "^5.0.45"
  },
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0"
  }
